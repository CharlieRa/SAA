body
  padding 50px
  font 14px "Lucida Grande", Helvetica, Arial, sans-serif

a
  color #00b7ff

#menu
  border-right 1px solid black
  height 100%
  padding-right 10px

.code
  width 200px
  height auto
  float left

.place
  width 200px
  height auto
  float left

.info
  width 70%
  height auto
  float left

.comprar
  width 30%
  height 100%
  float left
  postion relative
  padding-top 28px

#panel
  padding-left 30px

.autocomplete-suggestions
  border 1px solid #999
  background #FFF
  overflow auto
  strong
    font-weight normal
    color #3399FF

.autocomplete-suggestion
  padding 2px 5px
  white-space nowrap
  overflow hidden

.autocomplete-selected
  background #F0F0F0

#fecha
  &::-webkit-inner-spin-button
    -webkit-appearance none 

.info.page-header
  float left
