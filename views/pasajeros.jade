extends admin

block entidad
	h1.page-header Pasajeros
	
	.container
		//button.btn.btn-primary(type) Crear Nueva Entidad
		h2.sub-header Listado de pasajeros
		.table-responsive
			table.table.table-striped
				thead
					tr

						th ID
						th Nombre
						th Edad
						th Sexo
						th Pin

				tbody
					- for (var i = 0; i < data.length; i++)
							tr
								td #{data[i].id}
								td #{data[i].nombre}
								td #{data[i].edad}
								td #{data[i].sexo}
								td #{data[i].pin}
					