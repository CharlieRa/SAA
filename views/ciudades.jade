extends admin

block entidad
		include layout	
		.row
			.col-sm-8
				input#search.form-control(type='search', value='', placeholder='Search')
		.container-fluid
				h2.sub-header Citys
				table.table.table-striped.table-bordered#table
					thead
						tr
							th City Code
							th Name
							th Country Code
							th Edit
							th Delete
					tbody
						- for (var i = 0; i < data.length; i++)
								tr
									td #{data[i].CODE}
									td #{data[i].NAME}
									td #{data[i].C_CODE}
									td(style='width:5px;')
										a(href="/ciudades/modificar/#{data[i].CODE}")
											span.glyphicon.glyphicon-edit
									td(style='cursor: pointer; width:5px;')
										a(onclick="confirmGetMessage('#{data[i].CODE}')")
											span.glyphicon.glyphicon-trash
			ul.pagination
				li
					a(href='#') «
				li.active
					a(href='#') 1
				li
					a(href='#') 2
				li
					a(href='#') 3
				li
					a(href='#') 4
				li
					a(href='#') 5
				li
					a(href='#') »							
